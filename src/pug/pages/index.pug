extends ../layouts/main.pug

block variables 
    -var title = "Main page";
    -
        menuAsideItems = [
            {"name": "Брови", "url" : "#"},
            {"name": "Глаза", "url" : "#"},
            {"name": "Губы", "url" : "#"},
            {"name": "Удаление татуажа", "url" : "#"},
            {"name": "Татуировки", "url" : "#"},
            {"name": "Пирсинг", "url" : "#"}
        ];

block content 

    .content-container.d-flex.flex-no-wrap
        nav.nav.nav-aside
            .nav-aside__wrapper.nav-aside__wrapper--pad
                .logo.logo--d-none
                    img(src="img/Logo.png" alt="Логотип")
                ul.nav__list.nav__list--bottom-line.nav__list--pad.mt-70
                    for item in menuAsideItems 
                        li.nav__item 
                            a(href="" + item.url class="nav__link")
                                | #{item.name}
                .contacts-menu                     
                    address.contacts-menu__item.contacts-menu__location
                        svg(width="24px" height="24px")
                            use(xlink:href="#icon-location")
                        | Иваново, ул. Жиделева 21 #[span(class="remark") (Дербенев Центр) офис 301]
                    address.contacts-menu__item.contacts-menu__phone
                        svg(width="24px" height="24px")
                            use(xlink:href="#icon-phone")
                        a(href="tel:+79621636644") 
                            | +7 (962) 163-66-44
                        span.remark
                            | Пн—Сб, 10:00 — 19:00
                    a(href="#" class="contacts-menu__item icon--dark") 
                        svg(width="24px" height="24px") 
                            use(xlink:href="#icon-insta")
                        | Antipinastudio
                .btn__wrapper
                    button(class="btn btn--light btn--size btn--mt" type="button")
                        | ЗАПИСЬ
                    button(class="btn btn--dark btn--size btn--d-none" type="button")
                        | бесплатная консультация

        main.content-container__main
            header.header
                .header__slider 
                    img(src="../img/big-size/portraite-main-page.jpg", alt="" class="img img-stopper")
                    img(src="../img/big-size/portraite-main-page.jpg", alt="" class="img img-stopper")
                    img(src="../img/big-size/portraite-main-page.jpg", alt="" class="img img-stopper")
                .contacts
                    .contacts__wrapper.contacts__wrapper--d-none
                        a(href="#" class="contacts__item icon icon--side-line") 
                            svg(width="16px" height="16px") 
                                use(xlink:href="#icon-insta")
                        address.contacts__item.contacts__location
                            svg(width="12px" height="16px") 
                                use(xlink:href="#icon-location")
                            | Иваново, ул. Жиделева 21 #[span(class="remark") (Дербенев Центр) офис 301]
                        address.contacts__item.contacts__phone
                            a(href="tel:+79621636644") 
                                | +7 (962) 163-66-44
                            span.remark
                                | Пн—Сб, 10:00 — 19:00
                    // Управление мобильной навигацией
                    nav.nav-mobile.nav-mobile--d-none
                        a(href="#" class="") 
                            svg(width="145px" height="36px") 
                                use(xlink:href="#logo-svg")
                        span(class="nav-toggle nav-toggle--d-none")
                            svg(class="ham hamRotate ham4" width="25" height="25" viewBox="30 30 40 40" id="nav-toggle")
                                path(class="line top" d="m 70,33 h -40 c 0,0 -8.5,-0.149796 -8.5,8.5 0,8.649796 8.5,8.5 8.5,8.5 h 20 v -20")
                                path(class="line middle" d="m 70,50 h -40")
                                path(class="line bottom" d="m 30,67 h 40 c 0,0 8.5,0.149796 8.5,-8.5 0,-8.649796 -8.5,-8.5 -8.5,-8.5 h -20 v 20")
                .header__title
                    h2.title.title--header.anim-item._anim-no-hide
                        | Подчеркнём неуловимое
                    p.subtitle.anim-item._anim-no-hide
                        | Подчеркнём притягательность и естественную #[br(class="br-d-none")] красоту вашего лица
